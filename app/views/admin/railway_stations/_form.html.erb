<%= form_with(model: [:admin, station]) do |form| %>
  <%= render 'common/errors', resource: station %>
  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="actions">
    <%= form.submit t('.submit', action: station.new_record? ? 'Створити' : 'Відредагувати')%>
  </div>
<% end %>
